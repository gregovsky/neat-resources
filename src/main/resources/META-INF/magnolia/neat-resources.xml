<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "module.dtd" >
<module>
  <name>neat-resources</name>
  <displayName>${project.name}</displayName>
  <description>${project.description}</description>
  <class>com.neatresults.mgnltweaks.resources.NeatResourcesModule</class>
  <versionHandler>com.neatresults.mgnltweaks.resources.setup.NeatResourcesModuleVersionHandler</versionHandler>
  <version>${project.version}</version>
  <components>
    <id>main</id>
    <component>
      <type>com.neatresults.mgnltweaks.resources.ResourcesTemplatingFunctions</type>
      <implementation>com.neatresults.mgnltweaks.resources.ResourcesTemplatingFunctions</implementation>
      <scope>singleton</scope>
    </component>
  </components>

  <dependencies>
    <dependency>
      <name>core</name>
      <version>5.4/*</version>
    </dependency>
    <dependency>
      <name>resource-loader</name>
      <version>5.4/*</version>
    </dependency>
    <dependency>
      <name>resources</name>
      <version>2.0/*</version>
    </dependency>
    <!--  Add other dependencies here, e.g the mte (magnolia templating essentials).
    <dependency>
      <name>mte</name>
      <version>0.7/*</version>
